{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "headers": [
                {
                    "key": "Authorization",
                    "value": "*"
                }
            ],
            "dest": "index.js"
        }
    ]
}